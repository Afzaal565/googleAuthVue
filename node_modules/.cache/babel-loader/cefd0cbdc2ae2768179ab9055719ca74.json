{"ast":null,"code":"import { inject } from 'vue';\nexport default {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n\n  data() {\n    return {\n      user: ''\n    };\n  },\n\n  methods: {\n    async handleSignIn() {\n      try {\n        const googleUser = await this.$gAuth.signIn(); // console.log(this.$gAuth.signIn);\n\n        if (!googleUser) {\n          return null;\n        }\n\n        this.user = googleUser.getBasicProfile().getEmail();\n      } catch (error) {\n        console.log(error);\n        return null;\n      }\n    },\n\n    async handleSignOut() {\n      try {\n        await this.$gAuth.signOut(); // console.log(this.$gAuth.signOut);\n\n        this.user = '';\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  },\n\n  setup() {\n    const Vue3GoogleOauth = inject('Vue3GoogleOauth');\n    return {\n      Vue3GoogleOauth\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAcA,SAASA,MAAT,QAAuB,KAAvB;AAEA,eAAe;AACbC,MAAI,EAAE,YADO;AAEbC,OAAK,EAAE;AACLC,OAAG,EAAEC;AADA,GAFM;;AAMbC,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE;AADD,KAAP;AAGD,GAVY;;AAYbC,SAAO,EAAE;AACP,UAAMC,YAAN,GAAqB;AACnB,UAAI;AACF,cAAMC,UAAS,GAAI,MAAM,KAAKC,MAAL,CAAYC,MAAZ,EAAzB,CADE,CAEF;;AAEA,YAAI,CAACF,UAAL,EAAiB;AACf,iBAAO,IAAP;AACF;;AAEA,aAAKH,IAAL,GAAYG,UAAU,CAACG,eAAX,GAA6BC,QAA7B,EAAZ;AACF,OATA,CASE,OAAOC,KAAP,EAAc;AACdC,eAAO,CAACC,GAAR,CAAYF,KAAZ;AACA,eAAO,IAAP;AACF;AAED,KAhBM;;AAiBP,UAAMG,aAAN,GAAsB;AACpB,UAAI;AACF,cAAM,KAAKP,MAAL,CAAYQ,OAAZ,EAAN,CADE,CAEF;;AAEA,aAAKZ,IAAL,GAAY,EAAZ;AACF,OALA,CAKE,OAAOQ,KAAP,EAAc;AACdC,eAAO,CAACC,GAAR,CAAYF,KAAZ;AACF;AACF;;AA1BO,GAZI;;AAyCbK,OAAK,GAAG;AACN,UAAMC,eAAc,GAAIpB,MAAM,CAAC,iBAAD,CAA9B;AAEA,WAAO;AACLoB;AADK,KAAP;AAGF;;AA/Ca,CAAf","names":["inject","name","props","msg","String","data","user","methods","handleSignIn","googleUser","$gAuth","signIn","getBasicProfile","getEmail","error","console","log","handleSignOut","signOut","setup","Vue3GoogleOauth"],"sourceRoot":"","sources":["C:\\Users\\premier\\Desktop\\my app\\web-app\\src\\components\\HelloWorld.vue"],"sourcesContent":["<template>\r\n  <div class=\"hello\">\r\n    <h1>{{ msg }}</h1>\r\n    \r\n    <h1>Is Initialized: {{ Vue3GoogleOauth.isInit }}</h1>\r\n    <h1>Is Authorized: {{ Vue3GoogleOauth.isAuthorized }}</h1>\r\n    <h2 v-if='user'>Logged in user: {{ user }}</h2>\r\n\r\n    <button @click='handleSignIn' :disabled='!Vue3GoogleOauth.isInit || Vue3GoogleOauth.isAuthorized'>Sign In</button>\r\n    <button @click='handleSignOut' :disabled='!Vue3GoogleOauth.isAuthorized'>Sign Out</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { inject } from 'vue';\r\n\r\nexport default {\r\n  name: 'HelloWorld',\r\n  props: {\r\n    msg: String\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      user: '',\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async handleSignIn() {\r\n      try {\r\n        const googleUser = await this.$gAuth.signIn();\r\n        // console.log(this.$gAuth.signIn);\r\n\r\n        if (!googleUser) {\r\n          return null;\r\n        }\r\n\r\n        this.user = googleUser.getBasicProfile().getEmail();\r\n      } catch (error) {\r\n        console.log(error);\r\n        return null;\r\n      }\r\n      \r\n    },\r\n    async handleSignOut() {\r\n      try {\r\n        await this.$gAuth.signOut();\r\n        // console.log(this.$gAuth.signOut);\r\n\r\n        this.user = '';\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    }\r\n  },\r\n\r\n  setup() {\r\n    const Vue3GoogleOauth = inject('Vue3GoogleOauth');\r\n\r\n    return {\r\n      Vue3GoogleOauth,\r\n    };\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\nh3 {\r\n  margin: 40px 0 0;\r\n}\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\nli {\r\n  display: inline-block;\r\n  margin: 0 10px;\r\n}\r\na {\r\n  color: #42b983;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}